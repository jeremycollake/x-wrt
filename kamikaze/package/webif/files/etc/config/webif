config webif general
	option firmware_name ""
	option firmware_version ""
	option firmware_subtitle "With X-Wrt Extensions"
	option device_name ""
	
config webif syslog
	option ipaddr ""
	option port ""
	option size ""
	option type ""
	option mark ""
	option file ""

config webif theme
	option id "xwrt"

config webif cron
	option enable "1"

config webif qos
	option show_advanced_rules "0"

config webif misc
	option opendns "0"
	
#This is to be used for webif-page until a uci library is written in C
lang="en"
